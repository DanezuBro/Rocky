@model Rocky_Models.OrderHeader

<div class="text-center">
    @if(@Model !=null)
    {
                    <h1 class="text-primary">Order created !</h1>
                    <p>Order #@Model.Id has been created successfully.</p>
    }
    else
    {
            <h1 class="text-primary">Inquiry Confirmation</h1>
           <p>We have received your Inquiry and someone from our team will contact you shortly !</p>
    }
    <img src="/images/patio.jpg" style="width:80%;" />
</div>